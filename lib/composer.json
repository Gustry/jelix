{
    "name": "jelix/for-classic-package",
    "type": "library",
    "description": "used to declare jelix dependencies for testapp or other packages",
    "license": "LGPL-2.1-or-later",
    "require": {
        "jelix/fakeserverconf": "1.0.*",
        "jelix/composer-module-setup": "0.*",
        "phpmailer/phpmailer": "^5.2.26",
        "jelix/castor": "1.0.*",
        "jelix/feeds-module": "^1.7.3",
        "jelix/inifile": "^2.5.0",
        "jelix/simplelocalization": "1.7.1",
        "jelix/wikirenderer-plugin": "^1.7.2",
        "symfony/console":"^3.2.0",
        "jelix/file-utilities": "^1.8.3",
        "jelix/version": "1.0.*",
        "defuse/php-encryption": "^2.2.1",
        "google/recaptcha": "^1.1.3"
    },
    "autoload": {
        "psr-0": { "jelix\\": "."
        },
        "psr-4": {
            "Jelix\\DevHelper\\": "jelix-scripts/DevHelper/",
            "Jelix\\Routing\\UrlMapping\\": "jelix/core/UrlMapping/",
            "Jelix\\Installer\\": "jelix/installer/Installer/",
            "Jelix\\Scripts\\": "jelix/Scripts/",
            "Jelix\\Core\\Infos\\": "jelix/core/Infos/",
            "Jelix\\Dependencies\\" : "jelix/Dependencies"
        },
        "classmap": [ ],
        "files": [ "jelix/init.php" ]
    },
    "replace": {
        "jelix/jelix":"self.version"
    },
    "minimum-stability": "stable",
    "extra" : {
        "jelix": {
            "modules-dir" : [
                "jelix-modules",
                "jelix-admin-modules"
            ],
            "plugins-dir" : [
                "jelix-plugins"
            ]
        }
    }
}
