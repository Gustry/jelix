<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Jelix 1.7</title>
    <parser>
        <target>__PARSER_CACHE__</target>
        <encoding>utf8</encoding>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
        </extensions>
    </parser>
    <transformer>
        <target>__TARGET_PATH__</target>
    </transformer>
    <transformations>
        <template name="responsive-twig"/>
    </transformations>
    <files>
        <directory>lib/jelix/</directory>
        <ignore>*.ini.php</ignore>
        <ignore>lib/jelix/checker.php</ignore>
        <ignore>lib/jelix/docs/</ignore>
        <ignore>lib/jelix/core-modules/jelix/install/*</ignore>
    </files>
</phpdoc>
